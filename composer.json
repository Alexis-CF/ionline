{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "actived/microsoft-teams-notifier": "^1.2",
        "barryvdh/laravel-dompdf": "^2.0",
        "doctrine/dbal": "^3.0",
        "firebase/php-jwt": "^6.8",
        "guzzlehttp/guzzle": "^7.7",
        "laminas/laminas-escaper": "^2.7",
        "laravel/framework": "^10.0",
        "livewire/livewire": "^3.0",
        "laravel/passport": "^11.8",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.0",
        "luecano/numero-a-letras": "^2.2",
        "maatwebsite/excel": "^3.1",
        "owen-it/laravel-auditing": "^13.5",
        "phpoffice/phpword": "^1.1",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.3",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-google-cloud-storage": "^2.2",
        "spatie/laravel-permission": "^5.10",
        "spatie/pdf-to-text": "^1.52",
        "stackkit/laravel-google-cloud-scheduler": "^2.0",
        "stackkit/laravel-google-cloud-tasks-queue": "^3.6",
        "tinymce/tinymce": "^7.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.20",
        "kitloong/laravel-migrations-generator": "^6.11",
        "laravel/pint": "^1.17",
        "laravel/sail": "^1.12",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
